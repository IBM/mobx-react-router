{
  "name": "@superwf/mobx-react-router",
  "description": "Keep your MobX state in sync with react-router",
  "main": "lib/index.js",
  "version": "7.0.0",
  "unpkg": "dist/mobx-react-router.js",
  "browser": "dist/mobx-react-router.js",
  "types": "./module/index.d.ts",
  "module": "./module/index.js",
  "scripts": {
    "lint": "eslint src/*.ts __test__/*.{j,t}s",
    "test": "jest",
    "test-watch": "jest --watch",
    "clean": "rimraf {dist,module,lib} -f",
    "build:rollup": "cross-env NODE_ENV=production npx rollup -c rollup.config.js",
    "build:module": "cross-env NODE_ENV=production tsc -p tsconfig.declaration.json",
    "build": "npm run build:rollup && npm run build:module",
    "prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "files": [
    "dist",
    "module",
    "lib",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/superwf/mobx-react-router.git"
  },
  "keywords": [
    "react",
    "react-router",
    "routing",
    "router",
    "mobx",
    "state",
    "store"
  ],
  "author": "Ali Sheehan-Dare - @im_alisd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alisd23/mobx-react-router/issues"
  },
  "homepage": "https://github.com/alisd23/mobx-react-router#readme",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^10.0.0",
    "cross-env": "^3.0.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.0.2",
    "jest": "^27.0.6",
    "mobx-react-lite": "^3.0.0",
    "react": "^17.0.1",
    "rimraf": "^2.5.4",
    "rollup": "^2.33.1",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^4.3.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.3.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "peerDependencies": {
    "mobx": "^6.0.0"
  },
  "dependencies": {
    "history": "^5.0.1",
    "mobx": "^6.3.2"
  }
}
